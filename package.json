{
  "name": "@mdapi-issues/template",
  "description": "Minimal working example to demonstrate a bug in DESCRIPTION",
  "private": true,
  "version": "0.0.0-development",
  "author": "Matthias Rolke @amtrack",
  "dependencies": {
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@types/chai": "4.3.1",
    "@types/mocha": "9.1.1",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "chai": "4.3.6",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "mocha": "10.0.0",
    "prettier": "2.6.2",
    "sfdx-cli": "7.154.0",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "./lib"
  ],
  "keywords": [
    "salesforce",
    "mdapi-issue",
    "minimum-working-example",
    "MWE"
  ],
  "license": "MIT",
  "main": "./lib/workaround.js",
  "scripts": {
    "build": "rm -rf lib && tsc -b",
    "develop": "bash scripts/develop.sh",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "posttest": "yarn lint",
    "prepack": "yarn build",
    "prepare": "yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "test:e2e": "mocha --forbid-only \"test/**/*.e2e-spec.ts\""
  }
}
